{
	"name": "argilo",
	"main": "dist/argilo.cjs.js",
	"module": "dist/argilo.esm.js",
	"jsnext:main": "dist/argilo.esm.js",
	"types": "dist/argilo.d.ts",
	"description": "Argilo",
	"version": "1.0.0",
	"files": [
		"dist",
		"doc",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"clean:dist": "rimraf dist",
		"clean:doc": "rimraf doc",
		"clean:cover": "rimraf coverage",
		"clean": "npm-run-all clean:*",
		"lint": "tslint -c tslint.json ./src/**/*.ts",
		"check": "tsc --noEmit",
		"dts": "dts-bundle-generator -o dist/argilo.d.ts src/index.ts",
		"mocha": "mocha --opts mocha.opts src/**/*.spec.ts",
		"unit": "cross-env NODE_ENV=test nyc mocha src/**/*.spec.ts",
		"unit:browser": "cross-env NODE_ENV=test karma start config/karma.config.js  ",
		"perf:browser": "cross-env NODE_ENV=test karma start config/karma.perf.config.js",
		"unit:sauce": "cross-env NODE_ENV=test karma start config/karma.sauce.config.js",
		"compile": "rollup -c --environment NODE_ENV:production",
		"compile:dev": "rollup -c --environment NODE_ENV:development",
		"doc": "typedoc --options typedoc.json",
		"codecov": "cat coverage/*/lcov.info | codecov",
		"build": "npm-run-all clean lint check unit compile dts doc",
		"build:ci": "npm-run-all build cover codecov",
		"perf": "karma start karma.perf.config.js",
		"precommit": "tslint -c tslint.json --fix"
	},
	"homepage": "https://github.com/tao-zeng/argilo",
	"author": "Tao Zeng <tao.zeng.zt@qq.com>",
	"bugs": {
		"url": "https://github.com/tao-zeng/argilo/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/tao-zeng/argilo.git"
	},
	"keywords": [
		"template",
		"data binding",
		"observe",
		"watch",
		"compontent"
	],
	"private": false,
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.3.4",
		"@babel/plugin-external-helpers": "^7.2.0",
		"@babel/plugin-proposal-class-properties": "^7.3.4",
		"@babel/plugin-proposal-decorators": "^7.3.0",
		"@babel/preset-env": "^7.3.4",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/register": "^7.0.0",
		"@types/benchmark": "^1.0.31",
		"@types/expect.js": "^0.3.29",
		"@types/mocha": "^5.2.6",
		"@types/node": "^11.9.6",
		"babel-plugin-fix-class-properties-uninitialized": "^1.1.6",
		"babel-plugin-istanbul": "^5.1.1",
		"babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
		"babel-plugin-transform-es3-property-literals": "^6.22.0",
		"babel-preset-minify": "^0.5.0",
		"babel-register-ts": "^7.0.0",
		"benchmark": "^2.1.4",
		"cross-env": "^5.2.0",
		"dts-bundle-generator": "^2.1.0",
		"esdoc": "^1.1.0",
		"esdoc-standard-plugin": "^1.0.0",
		"esdoc-typescript-plugin": "^1.0.1",
		"expect.js": "^0.3.1",
		"jsdom": "^13.2.0",
		"jsdom-global": "^3.0.2",
		"karma": "^4.0.1",
		"karma-bench": "^0.1.3",
		"karma-benchmark-reporter": "^0.1.1",
		"karma-chrome-launcher": "^2.2.0",
		"karma-coverage": "^1.1.2",
		"karma-expect": "^1.1.3",
		"karma-ie-launcher": "^1.0.0",
		"karma-mocha": "^1.3.0",
		"karma-rollup-preprocessor": "^7.0.0",
		"karma-sauce-launcher": "^2.0.2",
		"karma-spec-reporter": "0.0.32",
		"karma-transform-path-preprocessor": "0.0.3",
		"mocha": "^2.5.3",
		"npm-run-all": "^4.1.5",
		"nyc": "^13.3.0",
		"pre-commit": "^1.2.2",
		"prettier": "^1.16.4",
		"rimraf": "^2.6.3",
		"rollup": "^1.3.2",
		"rollup-plugin-babel": "^4.3.2",
		"rollup-plugin-commonjs": "^9.2.1",
		"rollup-plugin-gzip": "^2.2.0",
		"rollup-plugin-jscc": "^1.0.0",
		"rollup-plugin-json": "^3.1.0",
		"rollup-plugin-node-resolve": "^4.0.1",
		"rollup-plugin-progress": "^1.0.0",
		"rollup-plugin-prototype-minify": "^1.0.5",
		"rollup-plugin-terser": "^4.0.4",
		"rollup-plugin-visualizer": "^1.0.2",
		"ts-node": "^8.0.2",
		"tslib": "^1.9.3",
		"tslint": "^5.13.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-plugin-prettier": "^2.0.1",
		"tslint-sonarts": "^1.9.0",
		"typedoc": "^0.14.2",
		"typedoc-plugin-example-tag": "^1.0.2",
		"typedoc-plugin-external-module-name": "^2.0.0",
		"typescript": "^3.3.3"
	}
}
