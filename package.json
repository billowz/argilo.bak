{
	"name": "argilo",
	"main": "dist/argilo.cjs.js",
	"module": "dist/argilo.es.js",
	"jsnext:main": "dist/argilo.es.js",
	"description": "Argilo",
	"version": "1.0.0",
	"scripts": {
		"build": "npm run eslint && npm run unit && npm run compile && npm run build-docs",
		"build-ci": "flow init && npm run eslint && npm run unit --browsers PhantomJS && npm run compile && npm run cover --browsers PhantomJS && npm run codecov",
		"compile": "set NODE_ENV=production&& rollup -c",
		"eslint": "node ./node_modules/eslint/bin/eslint.js ./",
		"unit": "karma start karma.config.js",
		"perf": "karma start karma.perf.config.js",
		"cover": "karma start karma.cover.config.js",
		"sauce": "karma start karma.sauce.config.js",
		"codecov": "cat coverage/*/lcov.info | codecov",
		"build-docs": "jsdoc -c .jsdoc"
	},
	"homepage": "https://github.com/tao-zeng/argilo",
	"author": "Tao Zeng <tao.zeng.zt@qq.com>",
	"bugs": {
		"url": "https://github.com/tao-zeng/argilo/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/tao-zeng/argilo.git"
	},
	"keywords": [
		"template",
		"data binding",
		"observe",
		"watch",
		"compontent"
	],
	"private": false,
	"license": "MIT",
