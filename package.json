{
  "name": "argilo",
  "namespace": "argilo",
  "main": "src/index.js",
  "dist": "dist/argilo.js",
  "description": "Argilo",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint src build",
    "package": "node build/build.js",
    "dev-unit": "karma start build/karma.config.js --browsers Chrome --no-single-run",
    "unit": "karma start build/karma.config.js --browsers Chrome",
    "ci-unit": "karma start build/karma.config.js",
    "cover": "karma start build/karma.cover.config.js --browsers Chrome",
    "ci-cover": "karma start build/karma.cover.config.js",
    "build": "npm run lint && npm run unit && npm run package",
    "ci-build": "npm run lint && npm run ci-unit && npm run package",
    "ci-codecov": "npm run ci-cover && cat coverage/*/lcov.info | codecov",
    "sauce": "karma start build/karma.sauce.config.js",
    "ci": "npm run ci-build && npm run ci-codecov",
    "server": "node build/server.js"
  },
  "homepage": "https://github.com/tao-zeng/argilo",
  "author": "Tao Zeng <tao.zeng.zt@gmail.com>",
  "bugs": {
    "url": "https://github.com/tao-zeng/argilo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tao-zeng/argilo.git"
  },
  "keywords": [
    "template",
    "data binding",
    "observe",
    "watch",
    "compontent"
  ],
  "private": false,
  "license": "MIT",
  "dependencies": {
    "todomvc-common": "^1.0.2",
    "todomvc-app-css": "^2.0.4"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^1.0.3",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-preset-es2015-loose-rollup": "^7.0.0",
    "codecov": "^1.0.1",
    "eslint": "^1.3.1",
    "express": "^4.14.0",
    "karma": "^1.1.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-expect": "",
    "karma-istanbul": "^0.0.2",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-rollup-preprocessor": "^2.0.2",
    "karma-sauce-launcher": "^1.0.0",
    "karma-spec-reporter": "^0.0.26",
    "mocha": "2.3.4",
    "path": "^0.12.7",
    "phantomjs-prebuilt": "^2.1.7",
    "rollup": "^0.34.1",
    "rollup-plugin-alias": "^1.2.0",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-istanbul": "^1.0.0",
    "rollup-plugin-local-resolve": "^1.0.7",
