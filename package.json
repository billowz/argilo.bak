{
	"name": "argilo",
	"main": "dist/argilo.cjs.js",
	"module": "dist/argilo.es.js",
	"jsnext:main": "dist/argilo.es.js",
	"types": "dist/argilo.d.ts",
	"description": "Argilo",
	"version": "1.0.0",
	"files": [
		"dist",
		"doc",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"clean:dist": "rimraf dist",
		"clean:doc": "rimraf doc",
		"clean:cover": "rimraf coverage",
		"clean": "npm-run-all clean:*",
		"lint": "tslint -c tslint.json ./src/**/*.ts",
		"check": "tsc --noEmit",
		"dts": "dts-bundle-generator -o dist/argilo.d.ts src/index.ts",
		"mocha": "mocha --opts mocha.opts src/**/*.spec.ts",
		"unit": "cross-env NODE_ENV=test nyc mocha src/**/*.spec.ts",
		"unit:browser": "cross-env NODE_ENV=test karma start karma.config.js --browsers ",
		"perf:browser": "cross-env NODE_ENV=test karma start karma.perf.config.js --browsers ",
		"unit:sauce": "cross-env NODE_ENV=test karma start karma.sauce.config.js",
		"compile": "rollup -c --environment NODE_ENV:production",
		"compile:dev": "rollup -c --environment NODE_ENV:development",
		"doc": "typedoc --options typedoc.json",
		"codecov": "cat coverage/*/lcov.info | codecov",
		"build": "npm-run-all clean lint check unit compile dts doc",
		"build:ci": "npm-run-all build cover codecov",
		"perf": "karma start karma.perf.config.js",
		"precommit": "tslint -c tslint.json --fix"
	},
	"homepage": "https://github.com/tao-zeng/argilo",
	"author": "Tao Zeng <tao.zeng.zt@qq.com>",
	"bugs": {
		"url": "https://github.com/tao-zeng/argilo/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/tao-zeng/argilo.git"
	},
	"keywords": [
		"template",
		"data binding",
		"observe",
		"watch",
		"compontent"
	],
	"private": false,
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@babel/plugin-external-helpers": "^7.2.0",
		"@babel/plugin-proposal-class-properties": "^7.2.1",
		"@babel/plugin-proposal-decorators": "^7.2.2",
		"@babel/preset-env": "^7.2.0",
		"@babel/preset-typescript": "^7.1.0",
		"@babel/register": "^7.0.0",
		"@types/benchmark": "^1.0.31",
		"@types/expect.js": "^0.3.29",
		"@types/mocha": "^5.2.5",
		"@types/node": "^10.12.15",
		"babel-plugin-istanbul": "^5.1.0",
		"babel-plugin-minify-dead-code-elimination": "^0.5.0",
		"babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
		"babel-plugin-transform-es3-property-literals": "^6.22.0",
		"babel-register-ts": "^7.0.0",
		"benchmark": "^2.1.4",
		"cross-env": "^5.2.0",
		"dts-bundle-generator": "^2.0.0",
		"esdoc": "^1.1.0",
		"esdoc-accessor-plugin": "^1.0.0",
		"esdoc-brand-plugin": "^1.0.1",
		"esdoc-coverage-plugin": "^1.1.0",
		"esdoc-external-ecmascript-plugin": "^1.0.0",
		"esdoc-integrate-manual-plugin": "^1.0.0",
		"esdoc-integrate-test-plugin": "^1.0.0",
		"esdoc-lint-plugin": "^1.0.2",
		"esdoc-publish-html-plugin": "^1.1.2",
		"esdoc-standard-plugin": "^1.0.0",
		"esdoc-type-inference-plugin": "^1.0.2",
		"esdoc-typescript-plugin": "^1.0.1",
		"esdoc-undocumented-identifier-plugin": "^1.0.0",
		"esdoc-unexported-identifier-plugin": "^1.0.0",
		"expect.js": "^0.3.1",
		"git-pre-commit": "^2.1.4",
		"jsdom": "^13.1.0",
		"jsdom-global": "^3.0.2",
		"karma": "^3.1.4",
		"karma-bench": "^0.1.3",
		"karma-benchmark-reporter": "^0.1.1",
		"karma-chrome-launcher": "^2.2.0",
		"karma-coverage": "^1.1.2",
		"karma-expect": "^1.1.3",
		"karma-ie-launcher": "^1.0.0",
		"karma-mocha": "^1.3.0",
		"karma-rollup-preprocessor": "^6.1.1",
		"karma-sauce-launcher": "^2.0.2",
		"karma-spec-reporter": "0.0.32",
		"karma-transform-path-preprocessor": "0.0.3",
		"mocha": "^2.5.3",
		"npm-run-all": "^4.1.5",
		"nyc": "^13.1.0",
		"prettier": "^1.15.3",
		"rimraf": "^2.6.2",
		"rollup": "^0.67.4",
		"rollup-plugin-babel": "^4.1.0",
		"rollup-plugin-commonjs": "^9.2.0",
		"rollup-plugin-filesize": "^5.0.1",
		"rollup-plugin-gzip": "^2.0.0",
		"rollup-plugin-node-resolve": "^4.0.0",
		"rollup-plugin-progress": "^0.4.0",
		"rollup-plugin-prototype-minify": "^1.0.4",
		"rollup-plugin-re": "^1.0.7",
		"rollup-plugin-serve": "^0.6.0",
		"rollup-plugin-uglify": "^6.0.0",
		"rollup-plugin-visualizer": "^0.9.2",
		"ts-node": "^7.0.1",
		"tslib": "^1.9.3",
		"tslint": "^5.11.0",
		"tslint-config-prettier": "^1.17.0",
		"tslint-plugin-prettier": "^2.0.1",
		"tslint-sonarts": "^1.8.0",
		"typedoc": "^0.13.0",
		"typedoc-plugin-example-tag": "^1.0.2",
		"typedoc-plugin-external-module-name": "^1.1.3",
		"typescript": "^3.2.2"
	}
}
