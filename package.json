  "version": "0.0.1",
  "scripts": {
    "lint": "eslint src",
    "package": "gulp build",
    "dev-unit": "karma start karma.config.js --browsers Chrome --no-single-run",
    "unit": "karma start karma.config.js --browsers Chrome",
    "ci-unit": "karma start karma.config.js",
    "cover": "karma start karma.cover.config.js --browsers Chrome",
    "ci-cover": "karma start karma.cover.config.js",
    "build": "npm run lint && npm run unit && npm run package",
    "ci-build": "npm run lint && npm run ci-unit && npm run package",
    "ci-codecov": "npm run ci-cover && cat coverage/*/lcov.info | codecov",
    "sauce": "karma start karma.sauce.config.js",
    "ci": "npm run ci-build && npm run ci-codecov",
    "watch": "gulp watch"
  },
