		"babel-plugin-unimport": "^0.0.2",
		"benchmark": "^2.1.4",
		"codecov": "^3.1.0",
		"eslint": "^5.8.0",
		"eslint-config-prettier": "^3.1.0",
		"eslint-plugin-flowtype": "^3.2.0",
		"eslint-plugin-prettier": "^3.0.0",
		"flow-bin": "^0.86.0",
		"ink-docstrap": "^1.3.2",
