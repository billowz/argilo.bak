{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"source": {
		"include": ["src/helper/"],
		"includePattern": ".+\\.js(doc)?$"
	},
	"plugins": ["plugins/markdown", "node_modules/jsdoc-babel"],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
		"template": "node_modules/ink-docstrap/template",
		"encoding": "utf8",
		"destination": "./docs/",
		"recurse": true,
		"readme": "readme.md"
	}
}
